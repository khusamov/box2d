{
	"name": "khusamov-box2d",
	"description": "Исследование библиотеки box2d",
	"packageManager": "yarn@3.1.1",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"arkanoid": "yarn workspaces foreach --include 'arkanoid' run start",
		"arkanoid:build": "yarn workspaces foreach --include 'arkanoid' run build",
		"sample1": "yarn workspaces foreach --include 'sample1' run start",
		"base-types:build": "yarn workspaces foreach --include 'base-types' run build",
		"base-types:build:watch": "yarn workspaces foreach --include 'base-types' run build:watch",
		"inversion-of-control:build": "yarn workspaces foreach --include 'inversion-of-control' run build",
		"inversion-of-control:build:watch": "yarn workspaces foreach --include 'inversion-of-control' run build:watch",
		"anubis:build": "yarn workspaces foreach --include 'anubis-*' run build",
		"anubis:build:watch": "yarn workspaces foreach --include 'anubis-*' run build:watch",
		"anubis-data-storage:build": "yarn workspaces foreach --include 'anubis-data-storage' run build",
		"anubis-data-storage:build:watch": "yarn workspaces foreach --include 'anubis-data-storage' run build:watch",
		"anubis-deletion-system:build": "yarn workspaces foreach --include 'anubis-deletion-system' run build",
		"anubis-deletion-system:build:watch": "yarn workspaces foreach --include 'anubis-deletion-system' run build:watch",
		"anubis-game-system-2:build": "yarn workspaces foreach --include 'anubis-game-system-2' run build",
		"anubis-game-system-2:build:watch": "yarn workspaces foreach --include 'anubis-game-system-2' run build:watch",
		"anubis-message-broker:build": "yarn workspaces foreach --include 'anubis-message-broker' run build",
		"anubis-message-broker:build:watch": "yarn workspaces foreach --include 'anubis-message-broker' run build:watch",
		"anubis-physic-system:build": "yarn workspaces foreach --include 'anubis-physic-system' run build",
		"anubis-physic-system:build:watch": "yarn workspaces foreach --include 'anubis-physic-system' run build:watch",
		"anubis-rule-system:build": "yarn workspaces foreach --include 'anubis-rule-system' run build",
		"anubis-rule-system:build:watch": "yarn workspaces foreach --include 'anubis-rule-system' run build:watch"
	}
}
